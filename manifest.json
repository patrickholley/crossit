{
  "manifest_version": 2,
  "name": "Crossit",
  "version": "1.0",
  "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
          "48": "placeholder.png"
      }
  },
  "description": "Crosspost saved posts to your own personal subreddit!",
  "icons": {
    "48": "placeholder.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}